{
  "name": "tiny-koa-graphql",
  "description": "A tiny koa-graphql server with tsup bundler",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node build/index.js",
    "start:dev": "tsup --onSuccess \"yarn start\"",
    "dev": "tsup --watch --onSuccess \"yarn start\"",
    "build": "tsup"
  },
  "main": "src/index.ts",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "dependencies": {
    "graphql": "15.5.0",
    "graphql-playground-middleware": "^1.1.2",
    "koa": "2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-convert": "^2.0.0",
    "koa-cors": "^0.0.16",
    "koa-graphql": "^0.8.0",
    "koa-router": "10.0.0"
  },
  "devDependencies": {
    "@types/koa": "2.13.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-convert": "^1.2.3",
    "@types/koa-cors": "^0.0.0",
    "@types/koa-graphql": "^0.8.4",
    "@types/koa-router": "7.4.2",
    "babel-loader": "8.2.2",
    "tsup": "^5.10.1",
    "typescript": "4.2.4"
  }
}
